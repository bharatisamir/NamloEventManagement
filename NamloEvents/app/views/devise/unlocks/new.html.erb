<div class="container" id="unlock_container">
  <div class="page-header">
    <h2>Didn't receive unlock instructions ? We are happy to resend them</h2>
  </div><!-- end page-header -->

  <div class="row">
    <div class="col-sm-4 feature">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title" align="center">DJ Dinesh</h3>
        </div><!-- end panel-heading -->
        <%= image_tag("application/dinesh_dai1.jpg", :alt =>"Wedding Event", :class =>"img-circle") %>
        <p></p>
        <p align="center">"Let me help you get all of your guest to the dance floor and make them groove as if no body is watchin"</p>
      </div><!-- end panel -->
    </div><!-- end col-sm-4 -->

    <div class="col-sm-4">
      <div class="list-group">
        <div class="list-group-item">
          <%= form_for(resource, as: resource_name, url: unlock_path(resource_name), html: { method: :post }) do |f| %>
            <div class="error_message">
              <%= devise_error_messages! %>
            </div>

            <div class="form-group">
              <%= f.label :email %><br />
              <%= f.email_field :email, autofocus: true, class: 'form-control' %>
            </div>

            <div class="actions">
              <%= f.submit "Resend unlock instructions", class: 'btn btn-primary' %>
            </div>
          <% end %>
          <%= render "devise/shared/links" %>
      </div><!-- end list-group-item -->
      </div><!-- end list-group -->
    </div><!-- end col-sm-4 -->

    <div class="col-sm-4">
      <div class="panel panel-default">
        <div class="panel-heading">
          <h3 class="panel-title" align="center">Nadim</h3>
        </div><!-- end panel-heading -->
        <%= image_tag("application/nadim1.jpg", :alt =>"Wedding Event", :class =>"img-circle") %>
        <p></p>
        <p align="center">"Photos are windows to past. Make every moments of your past memorable by hiring the right photographer"</p>
      </div><!-- end panel -->
    </div><!-- end col-sm-4 -->
  </div><!-- end row -->
</div><!-- end unlock_container -->

<hr>
<div class="script">

</div>